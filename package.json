        "gpii-binder": "1.1.0",
        "gpii-express": "1.0.15",
        "gpii-handlebars": "2.1.0",
        "infusion": "3.0.0-dev.20200525T143422Z.f7f6aab.FLUID-6148",
        "kettle": "1.12.0"
    },
    "devDependencies": {
        "eslint": "7.0.0",
        "eslint-config-fluid": "1.4.0",
        "foundation-sites": "6.6.3",
        "gpii-grunt-lint-all": "1.0.7",
        "gpii-testem": "2.1.11",
        "grunt": "1.1.0",
        "handlebars": "4.7.6",
        "markdown-it": "10.0.0",
        "mkdirp": "1.0.4",
