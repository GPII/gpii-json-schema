{
    "name": "gpii-json-schema",
    "version": "2.1.3",
    "description": "Support validation of JSON within the Fluid and GPII ecosystems.",
    "main": "index.js",
    "scripts": {
        "pretest": "node node_modules/rimraf/bin.js coverage reports && node node_modules/mkdirp coverage reports",
